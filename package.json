{
  "name": "poi-preset-svg-sprite",
  "version": "4.0.0",
  "description": "Add svg-sprite support in Poi.",
  "main": "index.js",
  "repository": "https://github.com/weirongxu/poi-preset-svg-sprite.git",
  "author": "Weirong Xu <weirongxu.raidou@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "xo": {
    "space": 2,
    "semicolon": false,
    "extends": [
      "plugin:jest/recommended"
    ],
    "ignore": [
      "test/fixtures"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "quote-props": "off",
      "semi-style": [
        "error",
        "first"
      ],
      "valid-jsdoc": "off",
      "no-warning-comments": "off",
      "capitalized-comments": "off",
      "indent": [
        "error",
        2,
        {
          "MemberExpression": "off"
        }
      ]
    }
  },
  "scripts": {
    "docs": "documentation readme index.js -s API",
    "test": "xo && jest"
  },
  "devDependencies": {
    "documentation": "^11.0.1",
    "eslint-plugin-jest": "^22.7.2",
    "glob": "^7.1.2",
    "jest": "^24.8.0",
    "pify": "^4.0.1",
    "poi": "^12.7.0",
    "rimraf": "^2.6.3",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0"
  }
}
