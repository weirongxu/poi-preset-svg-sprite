{
  "name": "poi-preset-svg-sprite",
  "version": "2.0.0",
  "description": "Add svg-sprite support in Poi.",
  "main": "index.js",
  "repository": "https://github.com/weirongxu/poi-preset-svg-sprite.git",
  "author": "Weirong Xu <weirongxu.raidou@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "indent": [
        "error",
        2,
        {
          "MemberExpression": "off"
        }
      ]
    }
  },
  "scripts": {
    "docs": "documentation readme index.js -s API",
    "test": "xo && ava"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "documentation": "^6.2.0",
    "glob": "^7.1.2",
    "poi": "^9.1.4",
    "xo": "^0.20.3"
  },
  "dependencies": {
    "svg-sprite-loader": "^3.7.3",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0"
  }
}
